{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Population growth is slowing, but what is the problem?",
    "width": 800,
    "height": 400,
    "projection": {"type": "equalEarth"},
    "data": {
      "url": "https://raw.githubusercontent.com/NichoHusni/3179Homework/main/js/ne_110m.json",
      "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
    },
      "layer": [
        {
            "transform": [
              {
                "lookup": "properties.ADM0_A3",
                "from": {
                  "data": {
                    "url": "https://raw.githubusercontent.com/NichoHusni/Assignment2/main/Dataset/BirthRate.csv"
                  },
                  "key": "code",
                  "fields": ["birth_rate"]
                }
              }
            ],
            "mark": {"type": "geoshape"},
            "encoding": {
              "color": {
                "field": "birth_rate",
                "type": "quantitative"
              },
              "tooltip": [
                {"field": "properties.NAME", "type": "nominal", "title": "Country"},
                {"field": "birth_rate", "type": "quantitative"}
              ]
            }
          }
      ]
}